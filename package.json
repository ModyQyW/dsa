{
  "name": "@modyqyw/dsa",
  "version": "0.0.2",
  "description": "Data structures and algorithms with TypeScript.",
  "author": {
    "email": "wurui7@mail3.sysu.edu.cn",
    "name": "ModyQyW",
    "url": "https://modyqyw.github.io"
  },
  "private": false,
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.min.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.min.js",
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.min.js",
      "require": "./dist/index.cjs.min.js"
    },
    "./": "./"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/ModyQyW/dsa/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/dsa.git"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "yarn lint:json && yarn lint:markdown && yarn lint:script",
    "lint:json": "prettier ./**/*.json --write",
    "lint:markdown": "markdownlint . --fix",
    "lint:script": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "test": "jest --coverage",
    "dist": "yarn test && yarn build && npm publish"
  },
  "devDependencies": {
    "@commitlint/cli": "~12.0.0",
    "@modyqyw/fabric": "~1.17.0",
    "@rollup/plugin-node-resolve": "~11.2.0",
    "@types/jest": "~26.0.20",
    "@types/lodash-es": "~4.17.4",
    "@types/node": "~14.14.31",
    "eslint": "~7.20.0",
    "husky": "~4.3.7",
    "jest": "~26.6.3",
    "lint-staged": "~10.5.4",
    "lodash-es": "~4.17.21",
    "markdownlint-cli": "~0.26.0",
    "prettier": "~2.2.1",
    "rollup": "~2.39.1",
    "rollup-plugin-dts": "~2.0.1",
    "rollup-plugin-terser": "~7.0.2",
    "rollup-plugin-typescript2": "~0.30.0",
    "ts-jest": "~26.5.2",
    "typescript": "~4.2.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": "prettier --write",
    "*.markdown": "markdownlint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}
