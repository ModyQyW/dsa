{
  "name": "dsa",
  "version": "0.0.0",
  "description": "Data structures and algorithms with TypeScript.",
  "author": {
    "email": "wurui7@mail3.sysu.edu.cn",
    "name": "ModyQyW",
    "url": "https://modyqyw.github.io"
  },
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "lint": "yarn lint:json && yarn lint:markdown && yarn lint:script",
    "lint:json": "prettier ./**/*.json --write",
    "lint:markdown": "markdownlint . --fix",
    "lint:script": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix"
  },
  "dependencies": {
    "lodash-es": "~4.17.20"
  },
  "devDependencies": {
    "@commitlint/cli": "~11.0.0",
    "@modyqyw/fabric": "~1.11.1",
    "@types/lodash-es": "~4.17.4",
    "eslint": "~7.17.0",
    "husky": "~4.3.7",
    "lint-staged": "~10.5.3",
    "markdownlint-cli": "~0.26.0",
    "prettier": "~2.2.1",
    "typescript": "~4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": "prettier --write",
    "*.markdown": "markdownlint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}
