{
  "name": "dsa",
  "version": "0.0.0",
  "description": "Data structures and algorithms with TypeScript.",
  "author": {
    "email": "wurui7@mail3.sysu.edu.cn",
    "name": "ModyQyW",
    "url": "https://modyqyw.github.io"
  },
  "private": true,
  "license": "Apache-2.0",
  "files": ["dist"],
  "main": "./dist/index.cjs.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./": "./"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "yarn lint:json && yarn lint:markdown && yarn lint:script",
    "lint:json": "prettier ./**/*.json --write",
    "lint:markdown": "markdownlint . --fix",
    "lint:script": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@modyqyw/fabric": "~1.16.1",
    "@rollup/plugin-typescript": "~8.2.0",
    "@types/jest": "~26.0.20",
    "@types/lodash": "~4.14.168",
    "@types/node": "~14.14.31",
    "commitlint": "~11.0.0",
    "eslint": "~7.20.0",
    "husky": "~4.3.7",
    "jest": "~26.6.3",
    "lint-staged": "~10.5.4",
    "lodash": "~4.17.21",
    "markdownlint-cli": "~0.26.0",
    "prettier": "~2.2.1",
    "rollup": "~2.39.0",
    "rollup-plugin-dts": "~2.0.1",
    "rollup-plugin-terser": "~7.0.2",
    "ts-jest": "~26.5.1",
    "typescript": "~4.1.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": "prettier --write",
    "*.markdown": "markdownlint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}
